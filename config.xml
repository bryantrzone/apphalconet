<?xml version='1.0' encoding='utf-8'?>
<widget id="com.HALCONET" version="1.0.17" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HalcoNet</name>
    <description>
        Aplicaci√≥n HalcoNet
    </description>
    <author email="contacto@tractozone.com.mx" href="http://www.tractozone.com.mx">
            Grupo TractoZone
        </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon platform="android" qualifier="ldpi" src="res/icons/android/ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="res/icons/android/mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="res/icons/android/hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="res/icons/android/xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="res/icons/android/xxhdpi.png" />
    <icon platform="android" qualifier="xxxhdpi" src="res/icons/android/xxxhdpi.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <engine name="android" spec="^6.2.3" />
</widget>
